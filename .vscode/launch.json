  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome lh:3000",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
    },
    {
      "name": "Launch Brave (macOS) port=3000",
      "type": "pwa-chrome",
      "request": "launch",
      "runtimeExecutable": "/Applications/Brave Browser.app/Contents/MacOS/Brave Browser",
      // "port": 3000,
      "url": "http://localhost:3000",
      // "urlFilter": "http://localhost:*",
      "webRoot": "${workspaceFolder}",
    },
  ]
}